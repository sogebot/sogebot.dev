{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "without moderation",
            "cwd": "${workspaceFolder}/backend",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "run",
                "debug"
            ],
            "console": "internalConsole",
            "outputCapture": "std",
            "env": {
                "ENV": "development",
                "CORS": "http://localhost:3000",
                "NODE_ENV": "development",
                "DISABLE":"moderation",
                "DEBUG": "tmi.message"
            },
            "preLaunchTask": "yarn: build - backend",
            "sourceMaps": true,
            "port": 9229,
            "outFiles": ["${workspaceFolder}/backend/dest/**/*"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "WITH moderation",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "run",
                "debug"
            ],
            "console": "internalConsole",
            "outputCapture": "std",
            "env": {
                "ENV": "development",
                "CORS": "http://localhost:3000",
                "NODE_ENV": "development",
                "DEBUG": "tmi.message"
            },
            "preLaunchTask": "yarn: build - backend",
            "sourceMaps": true,
            "port": 9229,
            "outFiles": ["${workspaceFolder}/dest/**/*"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "WITH moderation and send messages",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "run",
                "debug"
            ],
            "console": "internalConsole",
            "outputCapture": "std",
            "env": {
                "ENV": "development",
                "CORS": "http://localhost:3000",
                "NODE_ENV": "development"
            },
            "preLaunchTask": "yarn: build - backend",
            "sourceMaps": true,
            "port": 9229,
            "outFiles": ["${workspaceFolder}/dest/**/*"]
        }
    ]
}